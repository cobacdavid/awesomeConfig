\documentclass{article}

\usepackage[ecran,unepage]{prof}

\pagecolor{black}

\newcommand{\ctrl}{\texttt{ctrl}}
\newcommand{\shift}{\texttt{shift}}
\newcommand{\win}{\texttt{win}}
\newcommand{\alt}{\texttt{alt}}
\newcommand{\touchef}[1]{\texttt{F#1}}
\newcommand{\touche}[1]{\texttt{#1}}
\newcommand{\raccourci}[2]{%%
  \noindent\begin{tabular}{|p{2.5cm}|p{5cm}|}
    \begin{minipage}[t]{1.0\linewidth}
      \begin{flushleft}#1\end{flushleft}
    \end{minipage}  
    &  
      \begin{minipage}[t]{1.0\linewidth}
        \noindent #2
      \end{minipage}
  \end{tabular}\\
}

\pagestyle{empty}

\begin{document}
\color{gray}
\raccourci{\win{} + \touchef{1}}{Bascule wibox}
\raccourci{\win{} + \touchef{2}}{Bascule racourcis}
\raccourci{\win{} + \touchef{3}}{Navigateur}
\raccourci{\win{} + \touchef{4}}{Emacs}
\raccourci{\win{} + \touchef{5}}{}

\medskip
\raccourci{\win{} + \alt{} + \uparrow}{Luminosité +}
\raccourci{\win{} + \alt{} + \downarrow}{Luminosité -}
\raccourci{\win{} + \rightarrow}{Vers tag droit}
\raccourci{\win{} + \leftarrow}{Vers tag gauche}

\medskip
\raccourci{\win{} + \touche{Entrée}}{Terminal}
\raccourci{\win{} + \touche{p}}{Barre menu}
\raccourci{\win{} + \touche{w}}{Menu souris}
\raccourci{\win{} + \touche{j}}{Bascule client}
\raccourci{\win{} + \touche{k}}{Bascule client}
\end{document}

